{
    "name": "the-last-of-guss",
    "private": true,
    "version": "1.0.0",
    "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
    "scripts": {
        "dev": "pnpm -r dev",
        "dev:react": "pnpm --filter frontend dev",
        "dev:vue": "pnpm --filter frontend-vue dev",
        "dev:node": "pnpm --filter backend dev",
        "prod:react": "pnpm install --frozen-lockfile && pnpm --filter frontend prod",
        "prod:vue": "pnpm install --frozen-lockfile && pnpm --filter frontend-vue prod",
        "prod:node": "pnpm install --frozen-lockfile && pnpm --filter backend prod",
        "mg": "pnpm --filter backend mg",
        "lint": "pnpm -r lint",
        "typecheck": "pnpm -r typecheck",
        "test": "pnpm -r test"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@tailwindcss/vite": "^4.1.11",
        "@types/node": "^24.0.15",
        "eslint": "^9.31.0",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^16.3.0",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^4.1.12",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^7.1.3"
    },
    "dependencies": {
        "axios": "^1.10.0"
    }
}
